{"12":[{"text":"as all my fathers","number":"h1"},{"text":"and give ear","number":"h238"},{"text":"for I am a stranger","number":"h1616"},{"text":"at my tears","number":"h1832"},{"text":"hold not thy peace","number":"h2790"},{"text":"O LORD","number":"h3068"},{"text":"unto my cry","number":"h7775"},{"text":"Hear","number":"h8085"},{"text":"with thee and a sojourner","number":"h8453"},{"text":"my prayer","number":"h8605"}],"5":[{"text":"man","number":"h120"},{"text":"vanity","number":"h1892"},{"text":"and mine age","number":"h2465"},{"text":"as an handbreadth","number":"h2947"},{"text":"my days","number":"h3117"},{"text":"is altogether","number":"h3605"},{"text":"is as nothing before thee verily every","number":"h3605"},{"text":"at his best state","number":"h5324"},{"text":"Behold thou hast made","number":"h5414"},{"text":"Selah","number":"h5542"}],"11":[{"text":"surely every man","number":"h120"},{"text":"man","number":"h376"},{"text":"is vanity","number":"h1892"},{"text":"thou makest his beauty","number":"h2530"},{"text":"dost correct","number":"h3256"},{"text":"to consume away","number":"h4529"},{"text":"Selah","number":"h5542"},{"text":"for iniquity","number":"h5771"},{"text":"like a moth","number":"h6211"},{"text":"When thou with rebukes","number":"h8433"}],"7":[{"text":"And now Lord","number":"h136"},{"text":"what wait","number":"h6960"},{"text":"I for my hope","number":"h8431"}],"3":[{"text":"then","number":"h227"},{"text":"the fire","number":"h784"},{"text":"burned","number":"h1197"},{"text":"spake","number":"h1696"},{"text":"me while I was musing","number":"h1901"},{"text":"was hot","number":"h2552"},{"text":"My heart","number":"h3820"},{"text":"I with my tongue","number":"h3956"},{"text":"within","number":"h7130"}],"6":[{"text":"Surely every man","number":"h376"},{"text":"not who shall gather","number":"h622"},{"text":"in vain","number":"h1892"},{"text":"walketh","number":"h1980"},{"text":"surely they are disquieted","number":"h1993"},{"text":"riches and kn