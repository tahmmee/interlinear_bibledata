{"6":[{"text":"shall perish","number":"h6"},{"text":"but the way","number":"h1870"},{"text":"the way","number":"h1870"},{"text":"knoweth","number":"h3045"},{"text":"For the LORD","number":"h3068"},{"text":"of the righteous","number":"h6662"},{"text":"of the ungodly","number":"h7563"}],"1":[{"text":"is the man","number":"h376"},{"text":"Blessed","number":"h835"},{"text":"in the way","number":"h1870"},{"text":"that walketh","number":"h1980"},{"text":"of sinners","number":"h2400"},{"text":"nor sitteth","number":"h3427"},{"text":"of the scornful","number":"h3887"},{"text":"in the seat","number":"h4186"},{"text":"nor standeth","number":"h5975"},{"text":"not in the counsel","number":"h6098"},{"text":"of the ungodly","number":"h7563"}],"2":[{"text":"doth he meditate","number":"h1897"},{"text":"But his delight","number":"h2656"},{"text":"of the LORD","number":"h3068"},{"text":"day","number":"h3119"},{"text":"and night","number":"h3915"},{"text":"and in his law","number":"h8451"},{"text":"is in the law","number":"h8451"}],"5":[{"text":"nor sinners","number":"h2400"},{"text":"in the judgment","number":"h4941"},{"text":"in the congregation","number":"h5712"},{"text":"of the righteous","number":"h6662"},{"text":"shall not stand","number":"h6965"},{"text":"Therefore the ungodly","number":"h7563"}],"3":[{"text":"of water","number":"h4325"},{"text":"also shall not wither","number":"h5034"},{"text":"that bringeth forth","number":"h5414"},{"text":"his leaf","number":"h5929"},{"text":"And he shall be like a tree","number":"h6086"},{"text":"and whatsoever he doeth","number":"h6213"},{"text":"in his season","number":"h6256"},{"text":"by the rivers","number":"h6388"},{"text":"his fruit","number":"h6529"},{"text":"shall prosper","number":"h6743"},{"text":"planted","number":"h8362"}],"4":[{"text":"are not so but are like the chaff","number":"h4671"},{"text":"driveth away","number":"h5086"},{"text":"which the wind","number":"h7307"},{"text":"The ungodly","number":"h7563"}]}